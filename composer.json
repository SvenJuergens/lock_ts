{
    "name": "svenjuergens/lock_ts",
    "description": "A simple write protection for TypoScript Templates.",
    "keywords": ["typo3", "extension", "Backend", "TypoScript Templates"],
    "homepage": "https://github.com/SvenJuergens/lock_ts",
    "authors": [
        {
            "name": "Sven JÃ¼rgens",
            "email": "typo3@blue-side.de"
        }
    ],
    "license": "GPL-2.0+",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "^8.7"
    },
    "replace": {
        "lock_ts": "self.version",
        "typo3-ter/lock_ts": "self.version"
    },
    "autoload": {
        "psr-4": {
            "SvenJuergens\\LockTs\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}